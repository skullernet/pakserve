{
    "Listen": ":8080",
    "ContentType": "application/x-quake2-data",
    "RefererCheck": "^quake2://",
    "PakWhiteList": [ "" ],
    "DirWhiteList": [
        "^(players|models|sprites|sound|maps|textures|env|pics)/",
        "^[\\w\\-]*[.]filelist$",
        "^[\\w\\-]+[.](pak|pkz)$"
    ],
    "SearchPaths": {
        "^/(baseq2/|openffa/|opentdm/)?": ["/home/user/quake2/baseq2"],
        "^/ctf/": ["/home/user/quake2/ctf", "/home/user/quake2/baseq2"]
    },
    "LogLevel": 2,
    "LogTimeStamps": true
}
